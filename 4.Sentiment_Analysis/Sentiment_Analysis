   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Grouping data by month and product category\n",
    "category_counts = data.groupby(['Month', 'Product Category']).size().unstack()\n",
    "\n",
    "# Ploting the grouped bar graph\n",
    "category_counts.plot(kind='bar', figsize=(12, 6))\n",
    "plt.xlabel('Month')\n",
    "plt.ylabel('Number of Product Categories')\n",
    "plt.title('Number of Product Categories per Month')\n",
    "plt.xticks(rotation=0)  # Rotate x-axis labels for better readability\n",
    "plt.legend(title='Product Category')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
